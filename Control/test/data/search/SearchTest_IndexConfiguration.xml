<xproperties>
    <xproperties>
        <entry>
            <key>summa.control.service.port</key>
            <value class="int">2784</value>
        </entry>
        <entry>
            <key>summa.control.service.registry.port</key>
            <value class="int">2767</value>
        </entry>

        <entry>
            <key>FilterPump.ChainName</key>
            <value class="string">Simple fagref index</value>
        </entry>
        <entry>
            <key>FilterControl.Chains</key>
            <value class="string">SingleChain</value>
        </entry>
        <entry>
            <key>FilterControl.Sequential</key>
            <value class="boolean">true</value>
        </entry>
        <entry>
            <key>SingleChain</key>
            <value class="xproperties">
                <entry>
                    <key>FilterPump.Filters</key>
                    <value class="string">StorageReader, FagrefTransformer, OldToNewTransformer, DocumentCreator, IndexUpdate</value>
                </entry>
                <entry>
                    <key>StorageReader</key>
                    <value class="xproperties">
                        <entry>
                            <key>FilterPump.FilterClass</key>
                            <value class="string">dk.statsbiblioteket.summa.storage.filter.RecordReader</value>
                        </entry>
                        <entry>
                            <key>summa.storage.RecordReader.Storage</key>
                            <value class="string">//localhost:27000/TestStorage</value>
                        </entry>
                        <entry>
                            <key>summa.storage.RecordReader.start-from-scratch</key>
                            <value class="boolean">false</value>
                        </entry>
                        <entry>
                            <key>summa.storage.RecordReader.max-read.records</key>
                            <value class="int">-1</value>
                        </entry>
                        <entry>
                            <key>summa.storage.RecordReader.base</key>
                            <value class="string">fagref</value>
                        </entry>
                    </value>
                </entry>
                <entry>
                    <key>FagrefTransformer</key>
                    <value class="xproperties">
                        <entry>
                            <key>FilterPump.FilterClass</key>
                            <value class="string">dk.statsbiblioteket.summa.index.XMLTransformer</value>
                        </entry>
                        <entry>
                            <key>summa.xmltransformer.xslt</key>
                            <value class="string">/tmp/summatest/data/search/fagref_xslt/fagref_index.xsl</value>
                        </entry>
                    </value>
                </entry>
                <entry>
                    <key>OldToNewTransformer</key>
                    <value class="xproperties">
                        <entry>
                            <key>FilterPump.FilterClass</key>
                            <value class="string">dk.statsbiblioteket.summa.index.XMLTransformer</value>
                        </entry>
                        <entry>
                            <key>summa.xmltransformer.xslt</key>
                            <value class="string">LegacyToSummaDocumentXML.xslt</value>
                        </entry>
                    </value>
                </entry>
                <entry>
                    <key>DocumentCreator</key>
                    <value class="xproperties">
                        <entry>
                            <key>FilterPump.FilterClass</key>
                            <value class="string">dk.statsbiblioteket.summa.index.DocumentCreator</value>
                        </entry>
                        <entry>
                            <key>summa.index.descriptor-setup</key>
                            <value class="xproperties">
                                <entry>
                                    <key>summa.common.index-descriptor.absolute-location</key>
                                    <value class="string">/tmp/summatest/data/search/SearchTest_IndexDescriptor.xml</value>
                                </entry>
                            </value>
                        </entry>
                    </value>
                </entry>
                <entry>
                    <key>IndexUpdate</key>
                    <value class="xproperties">
                        <entry>
                            <key>FilterPump.FilterClass</key>
                            <value class="string">dk.statsbiblioteket.summa.index.IndexController</value>
                        </entry>
                        <entry>
                            <key>summa.index.manipulators</key>
                            <value class="string">LuceneUpdater</value>
                        </entry>
                        <entry>
                            <key>summa.index.create-new-index</key>
                            <value class="boolean">true</value>
                        </entry>
                        <entry>
                            <key>summa.index.index-root-location</key>
                            <value class="string">/tmp/summatest/testindex</value>
                        </entry>
                        <entry>
                            <key>summa.index.consolidate-timeout</key>
                            <value class="int">1000</value>
                        </entry>
                        <entry>
                            <key>LuceneUpdater</key>
                            <value class="xproperties">
                                <entry>
                                    <key>summa.index.manipulator-class</key>
                                    <value class="string">dk.statsbiblioteket.summa.index.LuceneManipulator</value>
                                </entry>
                                <entry>
                                    <key>summa.index.descriptor-setup</key>
                                    <value class="xproperties">
                                        <entry>
                                            <key>summa.common.index-descriptor.absolute-location</key>
                                            <value class="string">/tmp/summatest/data/search/SearchTest_IndexDescriptor.xml</value>
                                        </entry>
                                    </value>
                                </entry>
                            </value>
                        </entry>
                    </value>
                </entry>
            </value>
        </entry>
    </xproperties>
</xproperties>
